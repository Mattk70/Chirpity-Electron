{
  "name": "Chirpity",
  "version": "0.7.0",
  "description": "Chirpity Nocmig",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "export": "electron-builder build -w  --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattk70/Chirpity-Electron.git"
  },
  "build": {
    "files": [
      "**/*",
      "!test*${/*}",
      "!dist${/*}",
      "!24000_v9${/*}",
      "!*git*${/*}",
      "!package-lock.json",
      "!.idea",
      "node_modules/ffmpeg-static-electron/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static-electron/index.js",
      "node_modules/ffmpeg-static-electron/package.json",
      "node_modules/ffprobe-static-electron/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffprobe-static-electron/index.js",
      "node_modules/ffprobe-static-electron/package.json"
    ],
    "fileAssociations": [
      {
        "ext": "mp3",
        "name": "MP3 File",
        "role": "Editor"
      },
      {
        "ext": "wav",
        "name": "WAVE File",
        "role": "Editor"
      },
      {
        "ext": "ogg",
        "name": "WAVE File",
        "role": "Editor"
      },
      {
        "ext": "mpga",
        "name": "MPEG Audio File",
        "role": "Editor"
      },
      {
        "ext": "aac",
        "name": "AAC File",
        "role": "Editor"
      },
      {
        "ext": "mpa",
        "name": "MPA File",
        "role": "Editor"
      },
      {
        "ext": "flac",
        "name": "FLAC File",
        "role": "Editor"
      },
      {
        "ext": "mp4",
        "name": "MPEG4 File",
        "role": "Editor"
      }
    ],
    "mac": {
      "target": [
        "pkg"
      ],
      "files": [
        "!node_modules/ffmpeg-static-electron/bin/win${/*}",
        "!node_modules/ffmpeg-static-electron/bin/linux${/*}",
        "!node_modules/ffprobe-static-electron/bin/win${/*}",
        "!node_modules/ffprobe-static-electron/bin/linux${/*}"
      ],
      "icon": "./img/icon/icon.icns",
      "category": "public.app-category.utilities",
      "fileAssociations": [
        {
          "ext": [
            "wav",
            "mp3",
            "mpeg",
            "m4a",
            "aac",
            "mpga",
            "ogg",
            "mp4",
            "flac"
          ],
          "description": "Audio files",
          "role": "Viewer"
        }
      ]
    },
    "linux": {
      "icon": "./packages/img/icon/icon.icns",
      "target": [
        "AppImage"
      ],
      "files": [
        "!node_modules/ffmpeg-static-electron/bin/linux/ia32${/*}",
        "!node_modules/ffmpeg-static-electron/bin/win${/*}",
        "!node_modules/ffmpeg-static-electron/bin/mac${/*}",
        "!node_modules/ffprobe-static-electron/bin/linux/ia32${/*}",
        "!node_modules/ffprobe-static-electron/bin/win${/*}",
        "!node_modules/ffprobe-static-electron/bin/mac${/*}"
      ]
    },
    "win": {
      "asar": true,
      "icon": "./img/icon/icon.png",
      "files": [
        "node_modules/ffmpeg-static-electron/bin/win/${arch}/ffmpeg",
        "!node_modules/ffmpeg-static-electron/bin/win/ia32${/*}",
        "!node_modules/ffmpeg-static-electron/bin/linux${/*}",
        "!node_modules/ffmpeg-static-electron/bin/mac${/*}",
        "node_modules/ffprobe-static-electron/bin/win/${arch}/ffprobe",
        "!node_modules/ffprobe-static-electron/bin/win/ia32${/*}",
        "!node_modules/ffprobe-static-electron/bin/linux${/*}",
        "!node_modules/ffprobe-static-electron/bin/mac${/*}"
      ]
    }
  },
  "keywords": [
    "Nocmig",
    "Bioacoustics",
    "Bird Calls",
    "Birdsong",
    "DeepLearning",
    "Tensorflow"
  ],
  "author": "Matt Kirkland",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/mattk70/Chirpity-Electron/issues"
  },
  "homepage": "https://github.com/mattk70/Chirpity-Electron#readme",
  "devDependencies": {
    "electron": "^22.0.1",
    "electron-builder": "^23.6.0",
    "electron-wix-msi": "^4.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@tensorflow/tfjs": "^4.2.0",
    "bootstrap": "^5.2.2",
    "browser-id3-writer": "^4.4.0",
    "colormap": "^2.3.2",
    "electron-settings": "^4.0.2",
    "fast-png": "^6.1.0",
    "ffmpeg-static-electron": "^2.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "jquery": "^3.6.3",
    "seedrandom": "^3.0.5",
    "sqlite3": "^5.0.8",
    "suncalc2": "^1.8.1",
    "systeminformation": "^5.12.6",
    "utimes": "5.1.1",
    "uuid": "^8.3.2",
    "wavefile-reader": "^1.1.1",
    "wavesurfer.js": "^6.4.0"
  }
}
