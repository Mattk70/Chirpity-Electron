{
  "name": "Chirpity",
  "version": "0.6.0",
  "description": "Chirpity Nocmig",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "export": "electron-packager . --platform=all --arch=x64 --prune --out packages --overwrite --icon=/Users/matthew/PycharmProjects/Chirpity-Electron/img/icon/icon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattk70/Chirpity-Electron.git"
  },
  "build": {
    "fileAssociations": [
      {
        "ext": "chirp",
        "name": "Chirpity Results File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mp3",
        "name": "MP3 File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "wav",
        "name": "WAVE File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "ogg",
        "name": "WAVE File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mpga",
        "name": "MPEG Audio File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "aac",
        "name": "AAC File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mpa",
        "name": "MPA File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "flac",
        "name": "FLAC File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mp4",
        "name": "MPEG4 File",
        "role": "Editor",
        "perMachine": true
      }
    ],
    "mac": {
      "icon": "./packages/img/icon/icon.icns",
      "category": "public.app-category.utilities",
      "fileAssociations": [
        {
          "ext": [
            "wav",
            "mp3",
            "mpeg",
            "m4a",
            "aac",
            "mpga",
            "ogg",
            "chirp",
            "mp4",
            "flac"
          ],
          "description": "Audio files",
          "role": "Viewer"
        }
      ]
    }
  },
  "keywords": [
    "Nocmig",
    "Bioacoustics",
    "Bird Calls",
    "Birdsong",
    "DeepLearning",
    "Tensorflow"
  ],
  "author": "Matt Kirkland",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/mattk70/Chirpity-Electron/issues"
  },
  "homepage": "https://github.com/mattk70/Chirpity-Electron#readme",
  "devDependencies": {
    "electron": "^18.0.3",
    "electron-packager": "^15.4.0",
    "electron-wix-msi": "^3.2.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^3.13.0",
    "bootstrap": "^5.1.3",
    "browser-id3-writer": "^4.4.0",
    "colormap": "^2.3.2",
    "jquery": "^3.6.0",
    "lamejstmp": "^1.0.1",
    "node-gzip": "^1.1.2",
    "popper.js": "^1.16.1",
    "seedrandom": "^3.0.5",
    "suncalc2": "^1.8.1",
    "systeminformation": "^5.11.9",
    "uuid": "^8.3.2",
    "wavesurfer.js": "^6.2.0",
    "wavefile-reader": "^1.1.1",
    "sqlite3": "^5.0.8",
    "fluent-ffmpeg": "^2.1.2",
    "utimes": "^5.1.1"
  }
}
