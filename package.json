{
  "name": "chirpity",
  "version": "0.4.0",
  "description": "Chirpity Nocmig",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "export": "electron-packager . --platform=win32 --arch=x64 --prune --out packages --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattk70/Chirpity-Electron.git"
  },
  "build": {
    "fileAssociations": [
      {
        "ext": "chirp",
        "name": "Chirpity Results File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mp3",
        "name": "MP3 File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "wav",
        "name": "WAVE File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "ogg",
        "name": "WAVE File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mpga",
        "name": "MPEG Audio File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "aac",
        "name": "AAC File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mpa",
        "name": "MPA File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "flac",
        "name": "FLAC File",
        "role": "Editor",
        "perMachine": true
      },
      {
        "ext": "mp4",
        "name": "MPEG4 File",
        "role": "Editor",
        "perMachine": true
      }
    ]
  },
  "keywords": [
    "Nocmig",
    "Bioacoustics",
    "Bird Calls",
    "Birdsong",
    "DeepLearning",
    "Tensorflow"
  ],
  "author": "Matt Kirkland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattk70/Chirpity-Electron/issues"
  },
  "homepage": "https://github.com/mattk70/Chirpity-Electron#readme",
  "devDependencies": {
    "@tensorflow/tfjs-node-gpu": "^3.15.0",
    "electron": "^7.3.3",
    "electron-packager": "^14.2.1",
    "electron-wix-msi": "^3.2.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^3.13.0",
    "bootstrap": "^4.6.1",
    "browser-id3-writer": "^4.4.0",
    "colormap": "^2.3.2",
    "gpu-info": "^0.0.1",
    "jquery": "^3.6.0",
    "lamejstmp": "^1.0.1",
    "os": "^0.1.2",
    "popper.js": "^1.16.1",
    "seedrandom": "^3.0.5",
    "suncalc2": "^1.8.1",
    "update-electron-app": "^2.0.1",
    "uuid": "^8.3.2",
    "wavesurfer.js": "^6.0.4"
  }
}

